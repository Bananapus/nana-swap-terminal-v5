{
  "name": "@bananapus/swap-terminal",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bananapus/nana-swap-terminal"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "forge test",
    "test:fork": "FOUNDRY_PROFILE=CI forge test",
    "coverage": "forge coverage --match-path \"./src/*.sol\" --report lcov --report summary",
    "deploy:mainnets": "source ./.env && npx sphinx propose ./script/Deploy.s.sol --networks mainnets",
    "deploy:testnets": "source ./.env && npx sphinx propose ./script/Deploy.s.sol --networks testnets",
    "deploy:mainnets:usdc": "source ./.env && npx sphinx propose ./script/DeployUSDC.s.sol --networks mainnets",
    "deploy:testnets:usdc": "source ./.env && npx sphinx propose ./script/DeployUSDC.s.sol --networks testnets",
    "configure:mainnets": "source ./.env && npx sphinx propose ./script/ConfigurePairs.s.sol --networks mainnets",
    "configure:testnets": "source ./.env && npx sphinx propose ./script/ConfigurePairs.s.sol --networks testnets",
    "artifacts": "source ./.env && npx sphinx artifacts --org-id 'ea165b21-7cdc-4d7b-be59-ecdd4c26bee4' --project-name 'nana-swap-terminal'"
  },
  "dependencies": {
    "@bananapus/core": "^1.0.0",
    "@bananapus/permission-ids": "^0.0.17",
    "@exhausted-pigeon/uniswap-v3-foundry-pool": "^1.0.9",
    "@exhausted-pigeon/uniswap-v3-foundry-quote": "^1.0.0",
    "@openzeppelin/contracts": "^5.2.0",
    "@uniswap/permit2": "github:Uniswap/permit2",
    "@uniswap/v3-core": "github:Uniswap/v3-core#0.8",
    "@uniswap/v3-periphery": "github:Uniswap/v3-periphery#0.8"
  },
  "devDependencies": {
    "@sphinx-labs/plugins": "^0.33.1"
  }
}
